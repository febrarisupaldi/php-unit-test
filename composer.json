{
    "name": "paldi/php-unit-test",
    "description": "PHP Unit Test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Paldi\\Test\\": "src"
        }
    },
    "authors": [
        {
            "name": "Febrari Supaldi",
            "email": "febrarisupaldi@outlook.com"
        }
    ],
    "require": {},
	"require-dev" :{
		"phpunit/phpunit":"9.5.13"
	},
	"autoload-dev":{
		"psr-4":{
			"Paldi\\Test\\": "tests"
		}
	},
    "scripts":{
        "test" : "phpunit --configuration phpunit.xml",
        "test:product" : "phpunit --configuration phpunit.xml --testsuite product",
        "test:counter" : "phpunit --configuration phpunit.xml --testsuite counter"
    }
}
